#include<bits/stdc++.h>
using namespace std;
int main()
{
    vector<int> v(4);
    for(int i=0 ; i<4 ; i++)
    {
        cin >> v[i];
    }
    int c=0;
    sort(v.begin(),v.end());
    for(int i=0 ; i<4 ; i++)
    {
        if(v[i]==v[i+1])
        {
            c++;
        }
    }
    cout << c << endl;
}
